{
  "name": "polygit",
  "version": "0.1.0",
  "description": "URL rewriting polymer dev server",
  "main": "lib/bin/polygit.js",
  "repository": {
    "url": "https://github.com/Polymer/polygit.git",
    "type": "git"
  },
  "scripts": {
    "test": "npm run format && tsc && tslint -c tslint.json src/*.ts src/**/*.ts && mocha",
    "format": "find src | grep '\\.js$\\|\\.ts$' | xargs ./node_modules/.bin/clang-format --style=file -i",
    "start": "node lib/bin/polygit.js"
  },
  "author": "The Polymer Project Authors",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "chai": "^3.5.0",
    "clang-format": "^1.0.46",
    "source-map-support": "^0.4.6",
    "typescript": "^2.1.4"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "@types/chai": "^3.4.34",
    "@types/koa": "^0.0.0",
    "@types/memory-cache": "^0.0.29",
    "@types/mime": "^0.0.29",
    "@types/mocha": "^2.2.33",
    "@types/pegjs": "^0.10.0",
    "@types/request": "^0.0.36",
    "@types/semver": "^5.3.30",
    "@types/temp": "^0.8.29",
    "bower": "^1.8.0",
    "github": "^7.0.1",
    "gunzip-maybe": "^1.3.1",
    "json-stable-stringify": "^1.0.1",
    "koa": "^2",
    "memcached": "^2.2.2",
    "memory-cache": "^0.1.6",
    "mime": "^1.3.4",
    "mocha": "^3.2.0",
    "pegjs": "^0.10.0",
    "promisify": "^0.0.3",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3",
    "rimraf-promise": "^2.0.0",
    "semver": "^5.3.0",
    "source-map-support": "^0.4.6",
    "tar-fs": "^1.14.0",
    "temp": "^0.8.3"
  }
}
